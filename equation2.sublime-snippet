\[ 
	\int_0^\infty e^{-\alpha x^2} \mathrm{d}x = 
		\frac12\sqrt{\int_{-\infty}^\infty e^{-\alpha x^2}}
	\mathrm{d}x\int_{-\infty}^\infty e^{-\alpha y^2}\mathrm{d}y = 
		\frac12\sqrt{\frac{\pi}{\alpha}}
\]
]]></content>
